<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <properties resource="generator/generator.properties"/>

    <context id="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>

        <plugin type="${mapper.plugin}">
            <property name="mappers" value="${mapper.Mapper}"/>
        </plugin>

        <jdbcConnection driverClass="${jdbc.driverClass}"
                        connectionURL="${jdbc.url}"
                        userId="${jdbc.user}"
                        password="${jdbc.password}">
        </jdbcConnection>


        <!--【注意】如果同一工程中分多个模块，请手动修改包路径-->
        <javaModelGenerator targetPackage="com.bzn.robot.service.system.model"
                            targetProject="${targetJavaProject}"/>

        <sqlMapGenerator targetPackage="mapper" targetProject="${targetResourcesProject}"/>

        <javaClientGenerator targetPackage="com.bzn.robot.service.system.dao" targetProject="${targetJavaProject}"
                             type="XMLMAPPER"/>

